remotepasseval = mailpasswd("thiroc")
remotehost = imappro.zoho.com
remoteport = 993
sslcacertfile = /etc/ssl/certs/ca-certificates.crt

[Repository thiroc-local]
type = Maildir
localfolders = ~/.mail/thiroc
