[general]
accounts = thiroc
starttls = yes
ssl = yes
pythonfile = ~/.config/mutt/credentials/imappwd.py



[Account thiroc]
localrepository = thiroc-local
remoterepository = thiroc-remote

[Repository thiroc-remote]
